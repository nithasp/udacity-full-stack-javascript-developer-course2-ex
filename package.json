{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "Image resizing API with caching functionality",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "build:test": "npx tsc -p tsconfig.spec.json",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "npm run build:test && npm run jasmine",
    "test:unit": "npm run build:test && cross-env NODE_ENV=test jasmine --filter=unit",
    "test:integration": "npm run build:test && cross-env NODE_ENV=test jasmine --filter=integration",
    "jasmine": "cross-env NODE_ENV=test jasmine",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "image",
    "processing",
    "resize",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
